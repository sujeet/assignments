#include <iostream>
#include <string>
#include <string.h>
#include <fstream>

class Student{
     string id;
     string name;
     string address;
     string birth_date;
     float cgpa;
public:
     Student search_by_id (string id, int no_of_students, Student* archive )
	  {
	       int i = 0;
	       while (i<no_of_students){
		    if (archive[i].id == id){
			 return archive[i];
		    }
		    i++;
		    return Student("not found", "not found", "not found", "not found", 0);

